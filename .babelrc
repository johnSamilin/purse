{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "uglify": true,
      "exclude": ["transform-regenerator"],
      "useBuiltIns": true
    }]
  ],
  "plugins": [
    "transform-decorators-legacy",
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ]
}
