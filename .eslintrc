{
  "env": {
    "browser": true,
  },
	"extends": "airbnb",
	"globals": {
		"__APP_TYPE_CENTRAL__": true,
		"__APP_TYPE_NODE__": true,
	},
	"settings": {
		"import/resolver": {
			"webpack": { "config": "config/webpack.config.js" }
		}
	},
  "rules": {
      "no-confusing-arrow": ["error", {"allowParens": true}]
  }
}
