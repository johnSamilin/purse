{
  "name": "purse",
  "version": "0.2.4",
  "description": "purse",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "webpack-dev-server --config config/webpack.config.js --mode development",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server -- --open --https",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "author": "Alex Saltykov",
  "license": "MIT",
  "dependencies": {
    "abstract-leveldown": "^5.0.0",
    "currency-symbol-map": "^4.0.4",
    "d3": "^5.5.0",
    "d3-cluster": "^1.0.0-alpha.8",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "material-icons": "^0.1.0",
    "mobile-detect": "^1.4.1",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "offline-plugin": "^5.0.5",
    "pluralize": "^7.0.0",
    "pouchdb-adapter-http": "^6.4.3",
    "pouchdb-adapter-idb": "^6.4.3",
    "pouchdb-adapter-localstorage": "^7.0.0",
    "pouchdb-adapter-memory": "^7.0.0",
    "pouchdb-adapter-websql": "^7.0.0",
    "pouchdb-auth": "^2.1.1",
    "react": "^16.4.0",
    "react-bem-helper": "^1.4.1",
    "react-dom": "^16.4.0",
    "react-router": "^3.2.1",
    "react-simple-dropdown": "^3.2.0",
    "react-tabs": "^2.2.2",
    "rxdb": "^7.6.1",
    "rxjs": "^6.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "better-npm-run": "0.0.13",
    "compression": "^1.7.2",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "cssnano": "^3.7.4",
    "debug": "^2.6.9",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.9.0",
    "nodemon": "^1.15.0",
    "postcss-loader": "^2.1.5",
    "redbox-react": "^1.5.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "urijs": "^1.19.1",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.21.0",
    "yargs": "^6.3.0"
  }
}
